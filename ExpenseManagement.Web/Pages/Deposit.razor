@page "/app/deposits"

<PageTitle>Deposits</PageTitle>

<div class="card" style="width: 18rem;">
	<div class="card-header">
		<h3>
			<i class="bi bi-list-ul"></i> Deposit List
		</h3>
	</div>
	<div class="card-body">
		<table class="table">
			<thead>
				<tr>
					<th scope="col">#</th>
					<th scope="col">First</th>
					<th scope="col">Last</th>
					<th scope="col">Handle</th>
				</tr>
			</thead>
			<tbody>
				<tr>
					<th scope="row">1</th>
					<td>Mark</td>
					<td>Otto</td>
					
				</tr>
				<tr>
					<th scope="row">2</th>
					<td>Jacob</td>
					<td>Thornton</td>
					
				</tr>
				<tr>
					<th scope="row">3</th>
					<td>John</td>
					<td>Doe</td>
					
				</tr>
			</tbody>
		</table>
	</div>
</div>
